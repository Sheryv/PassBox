<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sheryv.PassCrypt"
    android:versionCode="1"
    android:versionName="1.1.93" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="22" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name="com.sheryv.PassCrypt.PassCryptApp"
        android:allowBackup="false"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Theme.AppCompat" >
        <activity
            android:name="com.sheryv.PassCrypt.PActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.sheryv.PassCrypt.DetailsActivity"
            android:label="@string/details" >
        </activity>
        <activity
            android:name="com.sheryv.PassCrypt.Options"
            android:label="@string/options" >
        </activity>
        <activity
            android:name="com.sheryv.PassCrypt.StartActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.sheryv.PassCrypt.AddActivity"
            android:label="@string/addNew" >
        </activity>
    </application>

</manifest>